{
  "name": "react-bootstrap-tab-navbar-functional-component",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "main": "src/index.js",
  "dependencies": {
    "axios": "^0.27.2",
    "bootstrap": "3.4.1",
    "formik": "^2.2.9",
    "react": "16.12.0",
    "react-bootstrap": "0.33.1",
    "react-dom": "16.12.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "3.0.1",
    "reactstrap": "^9.0.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "typescript": "3.3.3"
  },
  "eslintConfigOld": {
    "extendsOld": "react-app"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@samuelmeuli/eslint-config",
      "plugin:jsdoc/recommended"
    ],
    "env": {
      "node": true,
      "jest": true
    },
    "settings": {
      "jsdoc": {
        "mode": "typescript"
      }
    },
    "rules": {
      "no-await-in-loop": "off",
      "padded-blocks": "off",
      "no-useless-catch": "off",
      "no-unused-vars": [
        "error",
        {
          "args": "none",
          "varsIgnorePattern": "^_"
        }
      ],
      "jsdoc/check-indentation": "error",
      "jsdoc/check-syntax": "error",
      "jsdoc/newline-after-description": [
        "error",
        "never"
      ],
      "jsdoc/require-description": "error",
      "jsdoc/require-hyphen-before-param-description": "error",
      "jsdoc/require-jsdoc": "off"
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "cypress/",
    "public/",
    "build/",
    "src/libs/",
    "src/hooks/",
    "src/serviceWorker.js",
    "src/containers/",
    "src/components/"
  ],
  "jest": {
    "globalSetup": "./test/setup.js",
    "globalTeardown": "./test/teardown.js"
  },
  "prettier": "@samuelmeuli/prettier-config",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "react-scripts build && aws s3 sync build/ s3://paydhudmanbch",
    "deploy:prod": "export REACT_APP_STAGE=prod && react-scripts build && aws s3 sync build/ s3://davidhudman.com",
    "demo": "export REACT_APP_STAGE=demo",
    "prod": "export REACT_APP_STAGE=prod",
    "lint": "eslint --max-warnings 0 \"**/*.{js,js}\"",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --list-different \"**/*.{css,html,js,json,jsx,less,md,scss,ts,tsx,vue,yaml,yml}\"",
    "format:fix": "yarn format --write"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
